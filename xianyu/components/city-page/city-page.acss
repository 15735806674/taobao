.city{
    margin:0 20rpx;
    font-size:24rpx;
}
.citylist{
    margin:20rpx 0;
    flex-wrap: wrap;
    display: flex;
}
.citylist view{
    margin-top:20rpx;
    display: block;
    padding:0 25rpx;
    border:thin solid #ddd;
    height:60rpx;
    text-align: center;
    line-height: 60rpx;
    border-radius:10rpx;
    margin-right:10rpx;
    font-size:28rpx;
}